    "vim.statusBarColors.easymotioninputmode": ["#14161C", "#007ACC"],
    "vim.statusBarColors.surroundinputmode": ["#14161C", "#007ACC"],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<C-e>"],
            "after": [],
            "commands": [
                { "command": "workbench.action.toggleSidebarVisibility" }
            ]
        },
        {
          "before": ["<leader>", "d"],
          "after": ["d", "d"]
        },
        {
            "before": ["<leader>", "p"],
            "after": [],
            "commands": [
                { "command": "workbench.action.quickOpen" }
            ]
        }
    ],
    "vim.insertModeKeyBindings": [
        {
         "before": ["<C-h>"],
         "after": ["<LEFT>"]
        },
        {
         "before": ["<C-j>"],
         "after": ["<DOWN>"]
        },
        {
         "before": ["<C-k>"],
         "after": ["<UP>"]
        },
        {
         "before": ["<C-l>"],
         "after": ["<RIGHT>"]
        },
        {
         "before": ["j", "j"],
         "after": ["<Esc>"]
        },
        {
         "before": ["<C-a>"],
         "after": ["<esc>", "0", "i"]
        },
        {
         "before": ["<C-e>"],
         "after": ["<esc>", "$", "a"]
        }, 
    ],
    "terminal.integrated.fontSize": 16,
    "debug.console.fontSize": 16,
    "C_Cpp.updateChannel": "Insiders",
    "editor.minimap.side": "left",
    "git.confirmSync": false,
    "gitlens.sortBranchesBy": "date:desc",
    "gitlens.sortTagsBy": "date:desc",
    "gitlens.defaultDateSource": "committed",
    "gitlens.defaultDateStyle": "absolute",
    "search.exclude": {
        "{**/code.fo,**/code.allsym, **/tags, **/*.txt}": true,
        "doc/": true,
        "test/": true
    },
    "C_Cpp.exclusionPolicy": "checkFilesAndFolders",
    "python.formatting.autopep8Args": [
        "--indent-size=4",
        "--ignore=E402",
    ],
    "[markdown]": {
        "editor.tabSize": 4,                      // indent level to 4
        "editor.formatOnSave": true,              // at saving, format
        "editor.formatOnPaste": false,            // don't format on paste
        "editor.formatOnType": false,             // don't format on typing
    },
    "[python]": {
        "editor.tabSize": 4,                      // indent level to 4
        "editor.formatOnSave": true,              // at saving, format
        "editor.formatOnPaste": false,            // don't format on paste
        "editor.formatOnType": false,             // don't format on typing
    },
    "editor.fontSize": 15,
    "window.zoomLevel": -1,
    "cmake.configureOnOpen": false,
    "window.openFilesInNewWindow": "on",
    "editor.tabCompletion": "on",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.enablePreview": false,
    "vsicons.dontShowNewVersionMessage": true,
    "python.dataScience.askForKernelRestart": false,
    "python.dataScience.sendSelectionToInteractiveWindow": false,
    "gitlens.gitCommands.closeOnFocusOut": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "liveServer.settings.donotShowInfoMsg": true,
    "python.languageServer": "Microsoft",
    "workbench.iconTheme": "vscode-icons"
}