    "window.menuBarVisibility": "default",
    "vim.easymotion": true,
    "vim.easymotionKeys": "sdfghjkl",
    "vim.easymotionMarkerFontWeight": "bold",
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.camelCaseMotion.enable": true,
    "vim.leader": "<Space>",
