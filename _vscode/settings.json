{
    "workbench.colorTheme": "Dracula",
    "editor.fontFamily": "Myrica M for Powerline",
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.mouseWheelZoom": true,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "breadcrumbs.enabled": true,
    "terminal.integrated.profiles.windows": {
        "MSYS2 BASH" : {
            "path": [
                "D:\\msys64\\usr\\bin\\bash.exe",
            ],
            "args" : [
                "--login"
            ],
            "env": {
                "MSYSTEM": "MING ",
                "CHERE_INVOKING": "1"
            },
            "overrideName": true,
            "icon": "terminal-bash"
        }
    },
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.env.windows": {
        "MSYSTEM": "MINGW64",
        "CHERE_INVOKING": "1"
    },
    "terminal.integrated.fontFamily": "Myrica M for Powerline",
    "workbench.colorCustomizations": {
        "statusBar.background": "#14161C",
        "statusBar.noFolderBackground": "#14161C",
        "statusBar.debuggingBackground": "#14161C",
        "statusBar.foreground": "#DF71FB",
        "statusBar.debuggingForeground": "#DF71FB"
    },
    "files.associations": {
        // .clang-formatファイルをjsonに関連付ける
        ".clang-format": "json",
        // 拡張子の無いC++標準ライブラリのヘッダーをcppに関連付けする場合は以下のようにする
        // C/C++拡張機能が勝手にワークスペース設定に反映してくれるので設定をする必要はない
        "iostream": "cpp",
        "cstdlib": "cpp",
    },
    "C_Cpp.default.intelliSenseMode": "clang-x64",
    "C_Cpp.default.compilerPath": "D:\\msys64\\usr\\bin\\c++.exe",
    "C_Cpp.default.includePath": [
        "D:\\msys64\\usr\\include",
        "D:\\msys64\\usr\\lib\\clang\\3.8.0\\include",
        "D:\\msys64\\usr\\lib\\gcc\\x86_64-pc-msys\\7.3.0\\include",
        "D:\\msys64\\usr\\lib\\gcc\\x86_64-pc-msys\\7.3.0\\include\\c++",
        "D:\\msys64\\usr\\lib\\gcc\\x86_64-pc-msys\\7.3.0\\include\\cilk",
        "D:\\msys64\\usr\\lib\\gcc\\x86_64-pc-msys\\7.3.0\\include\\c++\\tr1"
    ],
    "git.path": "D:\\Program Files\\Git\\bin\\git.exe",
    "git.autofetch": true,
    "terminal.integrated.cursorBlinking": true,
    "sync.gist": "4b77e3ce5fb1db0f8748e410ac791854",
    "vim.foldfix": true,
    "window.menuBarVisibility": "visible",
    "vim.easymotion": true,
    "vim.easymotionMarkerFontWeight": "bold",
    "vim.highlightedyank.enable": true,
    "vim.visualstar": true,
    "vim.hlsearch": true,
    "vim.camelCaseMotion.enable": true,
    "vim.leader": "<Space>",
    "vim.useSystemClipboard": true,
    "vim.whichwrap": "h,l,<,>,[,]",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": ["#14161C", "#DF71FB"],
    "vim.statusBarColors.insert": ["#14161C", "#50FA7B"],
    "vim.statusBarColors.visual": ["#14161C", "#FCF404"],
    "vim.statusBarColors.visualline": ["#14161C", "#FEC5E5"],
    "vim.statusBarColors.visualblock": ["#14161C", "#00F9FF"],
    "vim.statusBarColors.replace": ["#14161C", "#D08770"],
    "vim.statusBarColors.commandlineinprogress": ["#14161C", "#007ACC"],
    "vim.statusBarColors.searchinprogressmode": ["#14161C", "#007ACC"],
    "vim.statusBarColors.easymotionmode": ["#14161C", "#007ACC"],
    "vim.statusBarColors.easymotioninputmode": ["#14161C", "#007ACC"],
    "vim.statusBarColors.surroundinputmode": ["#14161C", "#007ACC"],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<C-e>"],
            "after": [],
            "commands": [
                { "command": "workbench.action.toggleSidebarVisibility" }
            ]
        },
        // delete line
        {
            "before": ["<leader>", "d"],
            "after": ["d", "d"]
        },
        // open menu
        {
            "before": ["<leader>", "p"],
            "after": [],
            "commands": [
                { "command": "workbench.action.quickOpen" }
            ]
        },
        // Turn off highlight
        {
            "before": ["<Esc>", "<Esc>"],
            "commands": [":nohl"]
        },
        // Move focused tab to left
        { "before": [ "g", "r" ], "after": [ "g", "T" ] },
        { "before": [ "<leader>", "e" ], "commands": [ "workbench.view.explorer" ] },
        // Create new file
        { "before": [ "<leader>", "n" ], "commands": [ "workbench.action.files.newUntitledFile" ] },
        // Move current tab to left
        { "before": ["<leader>", "<"], "commands": [ ":tabm -1" ] },
        // Movw current tab to right
        { "before": ["<leader>", ">"], "commands": [ ":tabm +1" ] },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [">"],
            "after": [">", "g", "v"]
        },
        {
            "before": ["<"],
            "after": ["<", "g", "v"]
        }
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": ["<C-h>"],
            "after": ["<LEFT>"]
        },
        {
            "before": ["<C-j>"],
            "after": ["<DOWN>"]
        },
        {
            "before": ["<C-k>"],
            "after": ["<UP>"]
        },
        {
            "before": ["<C-l>"],
            "after": ["<RIGHT>"]
        },
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        },
        {
            "before": ["<C-a>"],
            "after": ["<esc>", "0", "i"]
        },
        {
            "before": ["<C-e>"],
            "after": ["<esc>", "$", "a"]
        },
    ],
    "terminal.integrated.fontSize": 16,
    "debug.console.fontSize": 16,
    "editor.minimap.side": "left",
    "git.confirmSync": false,
    "gitlens.sortBranchesBy": "date:desc",
    "gitlens.sortTagsBy": "date:desc",
    "gitlens.defaultDateSource": "committed",
    "gitlens.defaultDateStyle": "absolute",
    "search.exclude": {
        "{**/code.fo,**/code.allsym, **/tags, **/*.txt}": true,
        "doc/": true,
        "test/": true
    },
    "C_Cpp.exclusionPolicy": "checkFilesAndFolders",
    "python.formatting.autopep8Args": [
        "--indent-size=4",
        "--ignore=E402",
        "--aggressive",
        "--ignore=E50",
        "--max-line-length=120"
    ],
    "[markdown]": {
        "editor.tabSize": 4,                      // indent level to 4
        "editor.formatOnSave": true,              // at saving, format
        "editor.formatOnPaste": false,            // don't format on paste
        "editor.formatOnType": false,             // don't format on typing
    },
    "[python]": {
        "editor.tabSize": 4,                      // indent level to 4
        "editor.formatOnSave": true,              // at saving, format
        "editor.formatOnPaste": false,            // don't format on paste
        "editor.formatOnType": false,             // don't format on typing
    },
    "editor.fontSize": 15,
    "window.zoomLevel": -1,
    "window.openFilesInNewWindow": "on",
    "editor.tabCompletion": "on",
    "workbench.editor.enablePreview": false,
    "vsicons.dontShowNewVersionMessage": true,
    "gitlens.gitCommands.closeOnFocusOut": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "liveServer.settings.donotShowInfoMsg": true,
    "workbench.iconTheme": "vscode-icons",
    "vim.sneak": true,
    "vim.sneakReplacesF": true,
    "jupyter.askForKernelRestart": false,
    "jupyter.sendSelectionToInteractiveWindow": false,
    "workbench.editorAssociations":{
            "viewType": "jupyter-notebook",
            "filenamePattern": "jupyter-notebook"
    },
    "python.defaultInterpreterPath": "${workspaceFolder}\\venv\\Scripts\\python.exe",
    "python.condaPath": "$HOME\\AppData\\Local\\Continuum\\anaconda3",
    "python.venvPath": "${workspaceFolder}\\venv",
    "python.venvFolders": [
        "envs",
        ".pyenv",
        ".direnv"
    ],
    "python.linting.pylintEnabled": true,
    "python.linting.pylintPath": "pylint",
    "python.linting.flake8Enabled": true,
    "python.languageServer": "Pylance",
    "workbench.startupEditor": "none",
    "git-graph.defaultColumnVisibility": {

        "Date": true,
        "Author": true,
        "Commit": true
    },
    "indentRainbow.indicatorStyle": "light",
    "settingsSync.ignoredSettings": [

    ],
    "jupyter.interactiveWindowMode": "perFile",
}